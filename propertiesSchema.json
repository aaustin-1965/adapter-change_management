{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "url",
    "auth",
    "serviceNowTable"
  ],
  "properties": {
    "url": {
      "$id": "#/properties/url",
      "type": "string",
      "title": "The Url Schema",
      "default": "",
      "examples": [
        "https://yourDevServiceNowInstanceCanonicalHostname/"
      ],
      "pattern": "^(.*)$"
    },
    "auth": {
      "$id": "#/properties/auth",
      "type": "object",
      "title": "The Auth Schema",
      "required": [
        "username",
        "password"
      ],
      "properties": {
        "username": {
          "$id": "#/properties/auth/properties/username",
          "type": "string",
          "title": "The Username Schema",
          "default": "",
          "examples": [
            "devUsername"
          ],
          "pattern": "^(.*)$"
        },
        "password": {
          "$id": "#/properties/auth/properties/password",
          "type": "string",
          "title": "The Password Schema",
          "default": "",
          "examples": [
            "devUserPassword"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "serviceNowTable": {
      "$id": "#/properties/serviceNowTable",
      "type": "string",
      "title": "The Servicenowtable Schema",
      "default": "",
      "examples": [
        "change_request"
      ],
      "pattern": "^(.*)$"
    }
  }
}